                            {
                                "caption": "Settings – Default",
                                "command": "open_file", "args":
                                {
                                    "file": "${packages}/DocBlockr/Base File.sublime-settings"
                                }
                            },
                            {
                                "caption": "Settings – User",
                                "command": "open_file", "args":
                                {
                                    "file": "${packages}/User/Base File.sublime-settings"
                                }
                            },
                            {
                                "caption": "-"
                            },
                            {
                                "caption": "Key Bindings – Default",
                                "command": "open_file", "args":
                                {
                                    "file": "${packages}/DocBlockr/Default.sublime-keymap"
                                }
                            },
                            {
                                "caption": "Key Bindings – Default (OSX)",
                                "command": "open_file", "args":
                                {
                                    "file": "${packages}/DocBlockr/Default (OSX).sublime-keymap",
                                    "platform": "OSX"
